{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "array",
	"items": {
	"type": "object",
	"required": ["id", "name", "type", "caliber", "caliberMm"],
	"properties": {
		"id": {
			"type": "string",
			"pattern": "^[a-z0-9.-]+$"
		},
		"name": {
			"type": "string",
			"minLength": 3,
			"maxLength": 256
		},
		"type": {
			"type": "string",
			"enum": ["centerfire", "rimfire", "shotgun", "airgun"]
		},
		"manufacturerBullet": {
			"type": "string",
			"minLength": 2
		},
		"bulletName": {
			"type": "string"
		},
		"bulletWeight": {
			"type": "number",
			"minimum": 0
		},
		"bulletWeightUnit": {
			"type": "string",
			"enum": ["g", "gr"]
		},
		"bulletBc": {
			"type": "number",
			"minimum": 0,
			"maximum": 1
		},
		"bulletBcModel": {
			"type": "string",
			"enum": ["g1", "g7"]
		},
		"caliberId": {
			"type": "string"
		},
		"caliber": {
			"type": "string",
			"minLength": 2
		},
		"caliberMm": {
			"type": "number",
			"minimum": 0.1
		},
		"manufacturerPropellant": {
			"type": "string"
		},
		"propellantName": {
			"type": "string"
		},
		"propellantCharge": {
			"type": ["number", "null"],
			"minimum": 0
		},
		"propellantWeightUnit": {
			"type": "string",
			"enum": ["g", "gr"]
		},
		"primerType": {
			"type": "string",
			"enum": [
				"small rifle", 
				"small rifle magnum", 
				"large rifle", 
				"large rifle magnum", 
				"small pistol", 
				"small pistol magnum", 
				"large pistol", 
				"large pistol magnum", 
				"shotgun"
			]
		},
		"manufacturerPrimer": {
			"type": "string"
		},
		"primerName": {
			"type": "string"
		},
		"manufacturerCase": {
			"type": "string"
		},
		"cartridgeOverallLength": {
			"type": "number",
			"minimum": 0
		},
		"cartridgeOverallLengthUnit": {
			"type": "string",
			"enum": ["metric", "imperial"]
		},
		"note": {
			"type": "string"
		}
	},
	"additionalProperties": false
	}
}