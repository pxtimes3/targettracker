{
	"name": "targettracker",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "docker compose up -d && vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:start": "docker compose up -d",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"test:unit": "vitest --exclude \"**targettracker.analysis/**\"",
		"test": "npm run test:unit -- --run --coverage",
		"coverage": "vitest run --coverage",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.17.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.0.17",
		"@tailwindcss/vite": "^4.0.6",
		"@testing-library/jest-dom": "^6.6.3",
		"@types/eslint": "^9.6.1",
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.13.1",
		"@vitejs/plugin-basic-ssl": "^1.2.0",
		"@vitest/coverage-v8": "^3.0.5",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.30.6",
		"eslint": "^9.20.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"jsdom": "^26.0.0",
		"nodemailer": "^6.10.0",
		"pixi-filters": "^6.1.0",
		"postcss": "^8.5.2",
		"prettier": "^3.5.0",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.19.9",
		"svelte-check": "^4.1.4",
		"svelte-radix": "^2.0.1",
		"svelte-turnstile": "^0.10.0",
		"tailwindcss": "3",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.23.0",
		"uuid": "^11.0.5",
		"vite": "^6.1.0",
		"vitest": "^3.0.5",
		"zod": "^3.24.1"
	},
	"dependencies": {
		"@layerstack/tailwind": "^0.0.12",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sentry/sveltekit": "^9.0.0",
		"@types/nodemailer": "^6.4.17",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"@zxcvbn-ts/matcher-pwned": "^3.0.4",
		"drizzle-orm": "^0.39.2",
		"exifreader": "^4.26.1",
		"lucide-svelte": "^0.475.0",
		"luxon": "^3.5.0",
		"pixi.js": "^8.7.3",
		"postgres": "^3.4.5",
		"svelte-captcha-enhance": "^1.2.1",
		"svelte-sonner": "^0.3.28",
		"svelte-ux": "^0.90.2",
		"ts-luxon": "5.0.7-beta.0"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"exifreader",
			"@sentry/cli"
		],
		"onlyBuiltDependencies": [
			"exifreader",
			"@sentry/cli",
			"esbuild",
			"svelte-preprocess"
		]
	}
}
