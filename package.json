{
	"name": "targettracker",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "docker compose up -d && vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:start": "docker compose up -d",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"test:unit": "vitest --exclude \"**targettracker.analysis/**\"",
		"test": "npm run test:unit -- --run",
		"coverage": "vitest run --coverage",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "3.0.0-next.7",
		"@skeletonlabs/skeleton-svelte": "1.0.0-next.12",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.8.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@testing-library/jest-dom": "^6.6.3",
		"@types/eslint": "^9.6.1",
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.9.0",
		"@vitejs/plugin-basic-ssl": "^1.1.0",
		"@vitest/coverage-v8": "^2.1.5",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.28.0",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"jsdom": "^25.0.1",
		"nodemailer": "^6.9.16",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"svelte": "^5.1.13",
		"svelte-check": "^4.0.6",
		"svelte-radix": "^2.0.1",
		"svelte-turnstile": "^0.9.0",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.13.0",
		"uuid": "^11.0.2",
		"vite": "^5.4.10",
		"vitest": "^2.1.4",
		"zod": "^3.23.8"
	},
	"dependencies": {
		"@node-rs/argon2": "^2.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@types/nodemailer": "^6.4.16",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"@zxcvbn-ts/matcher-pwned": "^3.0.4",
		"drizzle-orm": "^0.36.1",
		"exifreader": "^4.25.0",
		"lucide-svelte": "^0.454.0",
		"luxon": "^3.5.0",
		"pixi.js": "^8.5.2",
		"postgres": "^3.4.5",
		"svelte-captcha-enhance": "^1.2.1",
		"svelte-sonner": "^0.3.28",
		"ts-luxon": "5.0.7-beta.0"
	}
}
