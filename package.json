{
	"name": "targettracker-data",
	"version": "1.0.0",
	"description": "Data files for Target Tracker",
	"scripts": {
		"test": "jest",
		"validate-calibers": "ajv validate -s schemas/caliber.json -d calibers.json",
		"validate-ammunition": "ajv validate -s schemas/ammunition.json -d ammunition.json",
