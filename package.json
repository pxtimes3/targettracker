{
	"name": "targettracker",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "docker compose up -d && vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:start": "docker compose up -d",
		"db:push": "drizzle-kit push",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "3.0.0-next.5",
		"@skeletonlabs/skeleton-svelte": "1.0.0-next.11",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.8.7",
		"autoprefixer": "^10.4.20",
		"drizzle-kit": "^0.27.0",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"nodemailer": "^6.9.16",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.1.6",
		"svelte-check": "^4.0.5",
		"svelte-radix": "^2.0.1",
		"svelte-turnstile": "^0.9.0",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2",
		"uuid": "^11.0.2",
		"vite": "^5.4.10",
		"vitest": "^2.1.4"
	},
	"dependencies": {
		"@node-rs/argon2": "^2.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@types/nodemailer": "^6.4.16",
		"@zxcvbn-ts/core": "^3.0.4",
		"@zxcvbn-ts/language-common": "^3.0.4",
		"@zxcvbn-ts/language-en": "^3.0.2",
		"@zxcvbn-ts/matcher-pwned": "^3.0.4",
		"drizzle-orm": "^0.36.0",
		"lucide-svelte": "^0.454.0",
		"luxon": "^3.5.0",
		"postgres": "^3.4.5",
		"svelte-captcha-enhance": "^1.2.1",
		"svelte-sonner": "^0.3.28",
		"ts-luxon": "5.0.7-beta.0"
	}
}
